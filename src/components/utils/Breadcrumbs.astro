---
import { Breadcrumbs as AstroBreadcrumbs } from "astro-breadcrumbs";
import "astro-breadcrumbs/breadcrumbs.css";
---

<div
  class="text-muted-foreground mb-2 flex items-center space-x-1 text-sm leading-none"
>
  
 
  <AstroBreadcrumbs separatorAriaHidden={false}>
    <svg
      slot="separator"
      xmlns="http://www.w3.org/2000/svg"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      fill="none"
      stroke="currentColor"
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"
      ><polyline points="9 18 15 12 9 6"></polyline>
    </svg>
  </AstroBreadcrumbs>
</div>
